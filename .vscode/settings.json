{
    "files.exclude": {
        "**/__pycache__": true,
        "**/wandb": true
    },
    "saveAndRun": {
        "commands": [
          {
            "match": ".*",
            "cmd": "bash config/rsync.sh",
            "useShortcut": false,
            "silent": true
          }
        ]
    },
    "python.terminal.activateEnvInCurrentTerminal": true
}